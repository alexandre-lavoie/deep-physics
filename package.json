  "scripts": {
    "start": "node ./build/index.js",
    "stage": "ts-node web/index.ts",
    "dev": "nodemon -x ts-node web/index.ts",
    "build": "node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandre-lavoie/deep-physics.git"
  },
  "author": "Alexandre Lavoie and James Kary",
