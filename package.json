  "bugs": {
    "url": "https://github.com/alexandre-lavoie/deep-physics/issues"
  },
  "homepage": "https://github.com/alexandre-lavoie/deep-physics#readme",
  "dependencies": {
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "@tensorflow/tfjs": "^2.3.0",
    "p5": "^1.1.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "cli-progress": "^3.8.2",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.4",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
